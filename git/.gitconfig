	a		 = !cd ${GIT_PREFIX:-.} && git add . && git s
	aa		 = !git add -A && git s
	c		 = commit
	d		 = diff
	ll		 = log --pretty=better-oneline --numstat
	ls		 = log --pretty=better-oneline
	s		 = status
	t 		 = "!bash /home/gbrl18/.dotfiles/bin/.local/scripts/h"
